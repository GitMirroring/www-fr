msgid "width=device-width, initial-scale=1"
msgstr "width=device-width, initial-scale=1"

#.  TRANSLATORS: Change direction to rtl if you translate
#.      the fundraiser and your script is right-to-left.  
#. type: Content of: <style>
#, no-wrap
msgid ""
"\n"
".fundraiser {\n"
"  direction: ltr;\n"
"}\n"
msgstr ""
"\n"
"@media (min-width:320px) {\n"
"  #fundraiser .message p.headline {\n"
"    font-size: .875em;\n"
"  }\n"
"  .message p.button {\n"
"    font-size: .875em;\n"
"    width: 40%;\n"
"  }\n"
"}\n"
"@media (min-width:435px) {\n"
"  #fundraiser .message p.headline {\n"
"    font-size: 1em;\n"
"  }\n"
"  .message p.button {\n"
"    font-size: 1em;\n"
"  }\n"
"}\n"
"@media (min-width:520px) {\n"
"  #fundraiser .message p.headline {\n"
"    font-size: 1em;\n"
"  }\n"
"  .message p.button {\n"
"    font-size: 1em;\n"
"  }\n"
"}\n"
"@media (min-width: 684px) {\n"
"  #fundraiser .message p.headline {\n"
"    text-align: left;\n"
"    font-size: 1em;\n"
"  }\n"
"  .message p.button {\n"
"    font-size: 1em;\n"
"  }\n"
"}\n"
"@media (min-width: 940px) {\n"
"  #fundraiser .message p.headline {\n"
"    font-size: 1.125em;\n"
"  }\n"
"  .message p.button {\n"
"    font-size: 1.125em;\n"
"  }\n"
"}\n"
"@media (max-width:683px) {\n"
"  #fundraiser .message p.headline span.suppress {\n"
"    display: none;\n"
"  }\n"
"  #fundraiser .message p.button {\n"
"    margin-left: auto; margin-right: auto;\n"
"    width: auto;\n"
"    font-size: 1em;\n"
"  }\n"
"}\n"
